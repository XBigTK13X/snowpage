{
  "expo": {
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/icon/android/tvbanner.png"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ]
    ],
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.simplepathstudios.snowpage"
    },
    "ios": {
      "bundleIdentifier": "com.simplepathstudios.snowpage"
    },
    "splash": {
      "image": "./assets/icon/splash.png",
      "backgroundColor": "#9ec0e6"
    },
    "newArchEnabled": true,
    "name": "snowpage",
    "slug": "snowpage",
    "scheme": "com.simplepathstudios.snowpage",
    "icon": "./assets/icon/appicon.png"
  }
}